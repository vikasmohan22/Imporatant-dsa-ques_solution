bool isPalindrome(int x){
        
        if (x<0)
        return false ;
        int temp=x;
        int r=0 ;
        double sum=0 ;
        while(x!=0)
        {
            r=x%10;
            sum=sum*10+r;
            x=x/10;
        } 
     if(sum==temp)
     return true ;
     else 
     return false ;
}
